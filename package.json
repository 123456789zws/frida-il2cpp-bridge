{
  "name": "frida-il2cpp-bridge",
  "version": "0.7.13",
  "description": "A Frida module to dump, trace or hijack any Il2Cpp application at runtime, without needing the global-metadata.dat file.",
  "keywords": [
    "frida",
    "il2cpp",
    "dump",
    "trace",
    "global-metadata"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vfsfitvnm/frida-il2cpp-bridge.git"
  },
  "license": "MIT",
  "author": "vfsfitvnm",
  "exports": "./dist/index.js",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "tsc && ccatjs dist/index.js dist/index.js --silent"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 160,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@types/frida-gum": "^18.3.1",
    "ccatjs": "^4.1.1",
    "typescript": "^5.0.4"
  }
}
